#include <stdio.h>
/**
  * main - Entry point
  *
  * Description: 'size of functions and what can do, mego'
  *
  * Return: Always 0 (Success)
  */
int main(void)
{
printf("Size of a char: %1u bytes(s)\n",sizeof(char));
printf("Size of an int: %1u bytes(s)\n",sizeof(int));
printf("Size of a long int: %1u bytes(s)\n",sizeof(long int));
printf("Size of a long long int: %1u bytes(s)\n",sizeof(long long int));
printf("Size of a float: %1u bytes(s)\n",sizeof(float));
return (0);
}
