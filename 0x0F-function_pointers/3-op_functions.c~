#include "3-calc.h"

/**
 * op_add - adding
 * @m: int1
 * @a: int2
 * Return: sum
 */
int op_add(int m, int a)
{
  return (a + b);
}

/**
 * op_sub - minasing
 * @m: int1
 * @a: int2
 * Return: sub
 */
int op_sub(int m, int a)
{
return (a - b);
}

/**
 * op_mul - producting
 * @m: int1
 * @a: int2
 * Return: product
 */

int op_mul(int m, int a)
{
return (a * b);
}

/**
 * op_div - diviaation
 * @m: int1
 * @a: int2
 * Return: diviation
 */
int op_div(int m, int a)
{
return (a / b);
}

/**
 * op_add - mod
 * @m: int1
 * @a: int2
 * Return: mod
 */
int op_mod(int m, int a)
{
return (a % b);
}
